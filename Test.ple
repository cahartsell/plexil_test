Command debugMsg( String msg );
Command driveToNextWaypoint();
Command update();

Boolean Lookup at_waypoint;

Main: Concurrence
{
  Boolean drive_done = false;

  DriveToWaypoint:
  {
    RepeatCondition drive_done == false;
    StartCondition Lookup(at_waypoint) == true;
    driveToNextWaypoint();
  }

  UpdateLoop:
  {
    RepeatCondition true;
    update();
  }

//  Bumper Triggered
